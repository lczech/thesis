    file = {:home/lucas/Dropbox/HITS/readings/mendeley/Logares et al., 2014, Metagenomic 16S rDNA Illumina tags are a powerful alternative to amplicon sequencing to explore diversity and stru.pdf:pdf},
    isbn = {14622912 (ISSN)},
    issn = {14622920},
    journal = {Environmental Microbiology},
    mendeley-groups = {Biological Data,Phylogenetics},
    month = {sep},
