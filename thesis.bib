    number = {1512},
    pages = {313--21},
    pmid = {12614582},
    publisher = {The Royal Society},
    title = {{Biological Identifications Through DNA Barcodes}},
    url = {http://www.ncbi.nlm.nih.gov/pubmed/12614582 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC1691236},
