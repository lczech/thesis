    file = {:home/lucas/Dropbox/HITS/readings/mendeley/Matsen, Gallagher, McCoy, 2013, Minimizing the average distance to a closest leaf in a phylogenetic tree.pdf:pdf},
    isbn = {1076-836X (Electronic)$\backslash$r1063-5157 (Linking)},
    issn = {10635157},
    journal = {Systematic Biology},
    mendeley-groups = {Phylogenetics},
