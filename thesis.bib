    issn = {1932-6203},
    journal = {PLOS ONE},
    keywords = {Algorithms,Cluster Analysis,Environmental Microbiology,Female,Humans,Metagenome,Metagenome: genetics,Phylogeny,Principal Component Analysis,Vagina,Vagina: microbiology,pca,placement},
    mendeley-groups = {Phylogenetics},
